<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!--
    author: Georges Alfaro S.
    version: 3.0.1
    library: simpleDBLib 3
    description: MySQL database properties
-->
<properties>
    <entry key="version">3.0</entry>
    <entry key="database_driver">com.mysql.cj.jdbc.Driver</entry>
    <entry key="protocol">jdbc:mysql:</entry>
    <entry key="server_url">localhost:3306</entry>
    <entry key="database">sample_db</entry>
    <entry key="user">root</entry>
    <entry key="password">root</entry>
    <entry key="JNDI_name">jdbc/sample_db</entry>
</properties>
